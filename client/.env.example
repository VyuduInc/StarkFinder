# StarkFinder Environment Variables
# SECURITY NOTES:
# 1. Never commit the actual .env file
# 2. Keep API keys secure and rotate them regularly
# 3. Use different API keys for development and production
# 4. Ensure all API keys have appropriate access restrictions

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/starkfinder"

# API Keys
OPENAI_API_KEY=your_openai_api_key
LAYERSWAP_API_KEY="your_layerswap_api_key_here"

# Network Configuration
NEXT_PUBLIC_STARKNET_NETWORK="mainnet" # or "testnet"
NEXT_PUBLIC_ETHEREUM_NETWORK="mainnet" # or "goerli"

# RPC URLs
STARKNET_RPC_URL="https://starknet-mainnet.infura.io/v3/your_infura_key"
NEXT_PUBLIC_ETHEREUM_RPC_URL="https://mainnet.infura.io/v3/your_infura_key"

# Bridge Configuration
NEXT_PUBLIC_MIN_BRIDGE_AMOUNT="0.001" # Minimum amount for bridging
NEXT_PUBLIC_MAX_BRIDGE_AMOUNT="10" # Maximum amount for bridging
NEXT_PUBLIC_DEFAULT_BRIDGE_TOKEN="ETH" # Default token for bridging

# Rate Limiting
RATE_LIMIT_REQUESTS=100 # Number of requests per window
RATE_LIMIT_WINDOW=60 # Window size in seconds

# Test Configuration
NEXT_PUBLIC_TEST_WALLET_ADDRESS="0x0123...your_test_wallet"

# Feature Flags
NEXT_PUBLIC_ENABLE_BRIDGING="true"
NEXT_PUBLIC_ENABLE_SWAP="true"

# Other configurations
BRIAN_API_KEY=""
RPC_URL ="https://free-rpc.nethermind.io/sepolia-juno/v0_7"
BOT_USERNAME=
MY_TOKEN = ''